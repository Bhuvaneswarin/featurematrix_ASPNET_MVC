@using Syncfusion.EJ2
<h3>Value, min and max properties: </h3>
<h5> This sample demonstrates the value, min and max properties of DateRangePicker component.</h5>
<br />
<div class="align">
    @Html.EJS().DateRangePicker("daterangepicker").Placeholder("Select a Date range").StartDate(ViewBag.startDate).EndDate(ViewBag.endDate).Render()
</div>

<style>
    .align {
        margin: 0 auto;
        width: 300px;
    }
</style>

